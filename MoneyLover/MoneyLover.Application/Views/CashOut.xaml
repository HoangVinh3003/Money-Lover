<Window x:Class="MoneyLover.Application.Views.CashOut"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoneyLover.Application.Views"
        mc:Ignorable="d"
        Title="CashOut" Height="298" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <TextBlock Text="RÚT TIỀN" FontSize="20" HorizontalAlignment="Center"  Margin="180,20,179,0"  />
        </Grid>
        <Grid Grid.Row="1" Margin="0,0,0,-10">
            <TextBox x:Name="txtCashOut" Width="300" Height="30" Margin="71,10,71,-7"  />
            <Button x:Name="btnCashOut" Click="btnCashOut_Click" Background="Green" Foreground="White" Content="RÚT TIỀN" Margin="71,72,71,-68" />
            <Button x:Name="btnCancel" Click="btnCancel_Click"  Content="HỦY" Margin="71,116,71,-112" />
        </Grid>
    </Grid>
</Window>
